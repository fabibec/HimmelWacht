{
    "configurations": [
        {
            "name": "Linux",
            // All this stuff is just to keep the intellisense happy
            // and is not needed for the actual build
            "includePath": [
                "${workspaceFolder}/**",
                "../esp-idf-v5.4.1/**",
                "${workspaceFolder}/components/interfaces/**",
                "${workspaceFolder}/components/drivers/**",
                "${workspaceFolder}/components/interfaces/diff-drive/include",
                "${workspaceFolder}/components/drivers/motor-driver/include",
                "../esp-idf-v5.4.1/components/driver/deprecated",
                "../esp-idf-v5.4.1/components/hal/include",
                "${workspaceFolder}/components/drivers/ds4-driver/include",
                "../bluepad32-v4.2.0/**",
                "../esp-idf-v5.4.1/components/freertos/FreeRTOS-Kernel-SMP/include",
                "../esp-idf-v5.4.1/components/freertos/config/include/freertos",
                "${workspaceFolder}/build/config",
                "../esp-idf-v5.4.1/components/freertos/config/linux/include",
                "../esp-idf-v5.4.1/components/freertos/FreeRTOS-Kernel-SMP/portable/linux/include/freertos",
                "../esp-idf-v5.4.1/components/esp_common/include",
                "../esp-idf-v5.4.1/components/freertos/FreeRTOS-Kernel-SMP/portable/linux/include",
                "../esp-idf-v5.4.1/components/freertos/esp_additions/include",
                "../esp-idf-v5.4.1/components/heap/include"
            ],
            "defines": [
                "ESP_PLATFORM",
                "CONFIG_FREERTOS_UNICORE=0",
                "I2C_CLK_SRC_DEFAULT=0"
            ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "gnu11",
            "cppStandard": "gnu++17",
            "intelliSenseMode": "linux-gcc-x64"
        }
    ],
    "version": 4
}
